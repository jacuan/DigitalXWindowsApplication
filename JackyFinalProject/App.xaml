<Application x:Class="JackyFinalProject.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:JackyFinalProject"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/PathStyle.xaml"/>
                <ResourceDictionary Source="Resources/TextBlockStyle.xaml"/>
                <ResourceDictionary Source="Resources/TextBoxStyle.xaml"/>
                <ResourceDictionary Source="Resources/PasswordBoxStyle.xaml"/>
                <ResourceDictionary Source="Resources/ButtonStyle.xaml"/>
                <ResourceDictionary Source="Resources/OverallBackgroundColor.xaml"/>
                <ResourceDictionary Source="Resources/LabelStyle.xaml"/>
                <ResourceDictionary Source="Resources/FrameStyle.xaml"/>
                <ResourceDictionary Source="Resources/ListBox.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
