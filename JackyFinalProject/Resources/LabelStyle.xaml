<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:JackyFinalProject">

    <Style x:Key="LabelStyle1" TargetType="Label">
        <Setter Property="Width" Value="410"/>
        <Setter Property="Height" Value="70"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FontSize" Value="28"/>
        <Setter Property="FontWeight" Value="Bold"/>
    </Style>

    <Style x:Key="LabelStyle2" TargetType="Label">
        <Setter Property="Width" Value="140"/>
        <Setter Property="Height" Value="25"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FontStyle" Value="Italic"/>
    </Style>
    
    <Style x:Key="LabelStyle3" TargetType="Label">
        <Setter Property="Width" Value="100"/>
        <Setter Property="Height" Value="40"/>
        <Setter Property="Foreground" Value="Azure"/>
    </Style>

    <Style x:Key="LabelStyle4" TargetType="Label">
        <Setter Property="Width" Value="100"/>
        <Setter Property="Height" Value="40"/>
        <Setter Property="Foreground" Value="Black"/>
    </Style>

    <Style x:Key="LabelStyle5" TargetType="Label">
        <Setter Property="Width" Value="200"/>
        <Setter Property="Height" Value="40"/>
        <Setter Property="Foreground" Value="Black"/>
    </Style>

</ResourceDictionary>